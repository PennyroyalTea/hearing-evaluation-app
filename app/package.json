{
  "name": "hearing-evaluation-app",
  "version": "0.0.2",
  "author": "Boris Starkov",
  "description": "App for hearing impaired people to take hearing tests & more",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PennyroyalTea/hearing-evaluation-app.git"
  },
  "bugs": {
    "url": "https://github.com/PennyroyalTea/hearing-evaluation-app/issues"
  },
  "private": true,
  "main": "src/backend-electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "asar": "^3.0.3",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.3",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "electron-build-all": "electron-builder -mwl",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "build-all": "yarn react-build && yarn electron-build-all",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5"
  },
  "build": {
    "extends": null,
    "files": [
      "build",
      "src/"
    ],
    "asar": false
  }
}
